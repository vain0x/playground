{
    "compilerOptions": {
        "baseUrl": ".",
        "esModuleInterop": true,
        "jsx": "react",
        "incremental": true,
        "lib": [
            "ES2019"
        ],
        "module": "CommonJS",
        "moduleResolution": "Node",
        "noEmitOnError": false,
        "noImplicitAny": true,
        "outDir": "target",
        "preserveWatchOutput": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "ES2019"
    }
}
